<div class="container-fluid">
  <div class="row">
    <div class="col-md-6">
      <div class="card-wrap">
        <div class="card">
          <div class="header">
              <h4 class="title">Environment Detail</h4>
              <h4>Name: {{model.name}}</h4>
              <h4>Description: {{model.description}}</h4>
          </div>
        </div>
      </div>
      <div class="card-wrap">
        <div class="card">
          <div class="header">
              <h4 class="title">Environment Values</h4>
              Your environment will be set as follows:

              {{#each-in model.json as |key val|}}
                <div class="row">
                  {{input type="text" value=key}} : {{input type="text" value=val}}
                  <!-- {{key}}={{val}} -->
                </div>
              {{/each-in}}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>



{{outlet}}
