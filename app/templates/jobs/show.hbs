<div class="card">
  <div class="header">
    <h4 class="title">Job Detail</h4>
    <div class="row">
      <div class="col-md-8">
        <h4>Name: {{model.name}}</h4>
        <h4>Frequency: Every {{model.interval.every}} {{model.interval.period}}</h4>
      </div>
      <div class="col-md-4">
        <button class="btn btn-default">{{model.last_result}}</button>
      </div>
    </div>
  </div>
  <div class="content">

  </div>
</div>

{{result-list title="Results" results=model.results}}
{{outlet}}
